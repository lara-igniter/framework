{
    "name": "lara-elegant/http",
    "description": "The Elegant Http package.",
    "license": "MIT",
    "homepage": "https://laraigniter.com",
    "support": {
        "issues": "https://github.com/lara-igniter/framework/issues",
        "source": "https://github.com/lara-igniter/framework"
    },
    "authors": [
        {
            "name": "George Tsachrelias",
            "email": "giorgosstab@laraigniter.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "lara-elegant/collections": "^1.0",
        "lara-elegant/macroable": "^1.0",
        "lara-elegant/contracts": "^1.0",
        "lara-elegant/support": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Elegant\\Http\\": ""
        }
    },
    "suggest": {
        "guzzlehttp/guzzle": "Required to use the HTTP Client (^6.5.5|^7.0.1).",
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}