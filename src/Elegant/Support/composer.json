{
    "name": "lara-elegant/support",
    "description": "The Elegant Support package.",
    "license": "MIT",
    "homepage": "https://laraigniter.com",
    "support": {
        "issues": "https://github.com/lara-igniter/framework/issues",
        "source": "https://github.com/lara-igniter/framework"
    },
    "authors": [
        {
            "name": "George Tsachrelias",
            "email": "giorgosstab@laraigniter.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-ctype": "*",
        "doctrine/inflector": "^1.4|^2.0",
        "lara-elegant/collections": "^1.0",
        "lara-elegant/conditionable": "^1.0",
        "lara-elegant/contracts": "^1.0",
        "lara-elegant/macroable": "^1.0",
        "ramsey/uuid": "^4.0",
        "vlucas/phpdotenv": "^5.4.1",
        "voku/portable-ascii": "^1.4.8"
    },
    "conflict": {
        "tightenco/collect": "<5.5.33"
    },
    "autoload": {
        "psr-4": {
            "Elegant\\Support\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "vlucas/phpdotenv": "Required to use the Env class and env helper (^5.4.1)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
