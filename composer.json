{
    "name": "lara-igniter/framework",
    "description": "The Extended Codeigniter 3 Framework.",
    "keywords": ["framework", "codeigniter", "extend"],
    "license": "MIT",
    "homepage": "https://g-tsachrelias.com",
    "support": {
        "issues": "https://github.com/lara-igniter/framework/issues",
        "source": "https://github.com/lara-igniter/framework"
    },
    "authors": [
        {
            "name": "George Tsachrelias",
            "email": "info@g-tsachrelias.com"
        }
    ],
    "require": {
        "php": "^7.2.5|^8.0",
        "ext-json": "*",
        "nesbot/carbon": "^2.31",
        "psr/container": "^1.0",
        "symfony/var-dumper": "^5.0",
        "vlucas/phpdotenv": "^4.0"
    },
    "replace": {
        "elegant/collections": "self.version",
        "elegant/contracts": "self.version",
        "elegant/macroable": "self.version",
        "elegant/support": "self.version"
    },
    "require-dev": {
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "mockery/mockery": "~1.3.3|^1.4.2",
        "phpunit/phpunit": "^8.4|^9.3.3"
    },
    "provide": {
        "psr/container-implementation": "1.0"
    },
    "conflict": {
        "tightenco/collect": "<5.5.33"
    },
    "autoload": {
        "files": [
            "src/Elegant/Collections/helpers.php"
        ],
        "psr-4": {
            "Elegant\\": "src/Elegant/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Elegant\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
